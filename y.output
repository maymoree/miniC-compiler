Grammar

    0 $accept: prog $end

    1 prog: extern_print extern_read func_def

    2 extern_read: EXTERN INT READ '(' ')' ';'

    3 extern_print: EXTERN VOID PRINT '(' INT ')' ';'

    4 func_def: INT ID '(' ')' block_stmt
    5         | INT ID '(' INT ID ')' block_stmt

    6 block_stmt: '{' var_decls stmts '}'
    7           | '{' stmts '}'

    8 var_decls: var_decls decl
    9          | decl

   10 decl: INT ID ';'

   11 stmts: stmts stmt
   12      | stmt

   13 stmt: ID '=' expr ';'
   14     | block_stmt
   15     | WHILE '(' cond ')' stmt
   16     | IF '(' cond ')' stmt ELSE stmt
   17     | IF '(' cond ')' stmt
   18     | PRINT '(' term ')' ';'
   19     | RETURN '(' expr ')' ';'
   20     | RETURN expr ';'

   21 cond: term GT term
   22     | term LT term
   23     | term EQ term
   24     | term GE term
   25     | term LE term
   26     | term NEQ term

   27 expr: term '+' term
   28     | term '-' term
   29     | term '*' term
   30     | term '/' term
   31     | term
   32     | READ '(' ')'

   33 term: NUM
   34     | ID
   35     | '-' term


